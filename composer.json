{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "api-platform/core": "^3.2",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^3.1.3",
        "elvi/api-log-bundle": "^2.0",
        "elvi/auth-bundle": "^5.0",
        "elvi/health-check-bundle": "^1.0",
        "ramsey/uuid": "^4.6",
        "sentry/sentry-symfony": "^5.0",
        "symfony/asset": "^7.0",
        "symfony/console": "^7.0",
        "symfony/dotenv": "^7.0",
        "symfony/expression-language": "^7.0",
        "symfony/flex": "^2.2",
        "symfony/framework-bundle": "^7.0",
        "symfony/messenger": "^7.0",
        "symfony/monolog-bundle": "^3.8",
        "symfony/property-access": "^7.0",
        "symfony/property-info": "^7.0",
        "symfony/security-bundle": "^7.0",
        "symfony/serializer": "^7.0",
        "symfony/translation": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/validator": "^7.0",
        "symfony/yaml": "^7.0"
    },
    "require-dev": {
        "codeception/codeception": "^5.0",
        "codeception/module-asserts": "^3.0",
        "codeception/module-doctrine": "^3.1",
        "codeception/module-rest": "^3.2",
        "codeception/module-symfony": "^3.0",
        "consolidation/robo": "^5.0",
        "friends-of-phpspec/phpspec-code-coverage": "^6.3",
        "mockery/mockery": "^1.6",
        "phpspec/phpspec": "^7.2",
        "phpunit/phpcov": "^9",
        "psalm/plugin-mockery": "^1.1",
        "roave/security-advisories": "dev-latest",
        "symplify/easy-coding-standard": "^12.0",
        "vimeo/psalm": "^5.6",
        "weirdan/doctrine-psalm-plugin": "^2.9"
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:vision-group-ag/elvi-auth-bundle.git"
        },
        {
            "type": "git",
            "url": "git@github.com:vision-group-ag/elvi-api-log-bundle.git"
        },
        {
            "type": "git",
            "url": "git@github.com:vision-group-ag/elvi-health-check-bundle.git"
        }
    ],
    "config": {
        "bin-dir": "bin",
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "composer/package-versions-deprecated": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTests\\": "tests/",
            "Spec\\App\\": "tests/Spec/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.0"
        }
    }
}
